  },
  "homepage": "https://github.com/shuked/MERN-eCommerce#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.7",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
